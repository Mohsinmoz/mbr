                })
            })

            window.CURRENT_SECTION = 'home'

            $(window).on('scroll', function (scroll) {
                var scrollTop = $(window).scrollTop()
                var section = 'home'
                if (scrollTop <= KEYS.home_disappear) {
                    section = 'home'
                } else if (scrollTop <= KEYS.timeline_disappear) {
                    section = 'timeline'
                } else if (scrollTop <= KEYS.vision_disappear) {
                    section = 'vision'
                } else if (scrollTop <= KEYS.channel_disappear) {
                    section = 'channel'
                } else if (scrollTop <= KEYS.publications_disappear) {
                    section = 'publications'
                } else if (scrollTop <= KEYS.grants_disappear) {
                    section = 'grants'
                } else if (scrollTop <= KEYS.conferences_disappear) {
                    section = 'conferences'
                } else {
                    TopMenu.hide()
                    BottomMenu.hide()
                    return
                }

                if (section == CURRENT_SECTION) {
                    return
                }

                CURRENT_SECTION = section
                switch (CURRENT_SECTION) {
                    case 'home':
                        window.location.hash = 'home'
                        activeMenu('home')
                        TopMenu.show()
                        BottomMenu.show()
                        break;
                    case 'timeline':
                        window.location.hash = 'timeline'
                        activeMenu('timeline')
                        TopMenu.hide()
                        BottomMenu.hide()
                        break;
                    case 'vision':
