                    document.getElementById('grantsId').click();
                }
                if ($('.menu.grants').hasClass("active")) {
                    document.getElementById('conferencesId').click();
                }
            }
            if (delta == 1) {
                if ($('.menu.timeline').hasClass("active")) {
                    document.getElementById('homeId').click();
                }
                if ($('.menu.vision').hasClass("active")) {
                    document.getElementById('timelineId').click();
                }
                if ($('.menu.channel').hasClass("active")) {
                    document.getElementById('visionId').click();
                }
                if ($('.menu.publications').hasClass("active")) {
                    document.getElementById('channelId').click();
                }
                if ($('.menu.grants').hasClass("active")) {
                    document.getElementById('publicationsId').click();
                }
                if ($('.menu.conferences').hasClass("active")) {
