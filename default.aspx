                        break;
                    case 'conferences':
                        window.location.hash = 'conferences'
                        activeMenu('conferences')
                        TopMenu.hide()
                        BottomMenu.hide()
                        break;
                }
            })

            function activeMenu(menu) {
                $("#top-menu .menus .menu").removeClass('active')
