    <script>
        $(document).ready(function () {
            var tag = document.createElement("script");
            tag.src = "http://www.youtube.com/iframe_api";
            var firstScriptTag = document.getElementsByTagName("script")[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

            $(".slides .video").click(function () {
                src = $(this).attr('data-src');
                onYouTubeIframeAPIReady(src);


                function onYouTubeIframeAPIReady(src) {
                    var player;
                    player = new YT.Player("player", {
                        width: 640,
                        height: 400,
                        videoId: src,
                        events: {                            
                            'onStateChange': onPlayerStateChange
                        }
                    });
                }
                function onPlayerStateChange(event) {
                    if (event.data === YT.PlayerState.ENDED) {
                        $(".video-light-box").css("display","none")
                        $("#player_wrap iframe").remove();
                        $('#player_wrap').append('<div id="player"></div>');
                    }
                }
            });

            $("#section-vision .do-close").click(function () {
                $("#player_wrap iframe").remove();
                $('#player_wrap').append('<div id="player"></div>');

            });

            $(".video-slides .video").click(function () {
                src = $(this).attr('data-src');
                onYouTubeIframeAPIReady(src);


                function onYouTubeIframeAPIReady(src) {
                    var player;
                    player = new YT.Player("player2", {
                        width: 640,
                        height: 400,
                        videoId: src,
                        events: {
                            'onStateChange': onPlayerStateChange
                        }
                    });
                }
                function onPlayerStateChange(event) {
                    if (event.data === YT.PlayerState.ENDED) {
                        $(".video-light-box").css("display", "none")
                        $("#player_wrap iframe").remove();
                        $('#player_wrap').append('<div id="player"></div>');
                    }
                }
            });

            $("#section-channel .do-close").click(function () {
                $("#player_wrap2 iframe").remove();
                $('#player_wrap2').append('<div id="player2"></div>');

            });
           
        });
    </script>

